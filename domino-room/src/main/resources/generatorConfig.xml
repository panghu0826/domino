<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 驱动包路径！ 慎用此功能，，会覆盖掉手动添加代码，，若使用请先备份文件，最好一次只生成一个表的，，否则后果很严重 -->
    <classPathEntry
            location="C:\Users\feide\.m2\repository\mysql\mysql-connector-java\5.1.20\mysql-connector-java-5.1.20.jar"/>
    <context id="MysqlTables" targetRuntime="MyBatis3Simple" defaultModelType="flat">
        <property name="mergeable" value="true"></property>
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- jdbc url地址 ！ -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://10.0.0.90:3306/jule_room?generateSimpleParameterMetadata=true"
                        userId="root" password="root">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- targetPackage 生成后的路径 ，targetProject 绝对路径  该配置生成java代码  -->
        <javaModelGenerator targetPackage="com.jule.room.dao.bean"
                            targetProject="C:\Users\feide\Desktop\code2\Product_Code\room-server\sourcecode\Server\trunk\jule-room\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 该配置生成 mybatis 映射文件   -->
        <sqlMapGenerator targetPackage="com.jule.room.dao.mapper"
                         targetProject="C:\Users\feide\Desktop\code2\Product_Code\room-server\sourcecode\Server\trunk\jule-room\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成 java 接口代码  -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.jule.room.dao.mapper"
                             targetProject="C:\Users\feide\Desktop\code2\Product_Code\room-server\sourcecode\Server\trunk\jule-room\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table schema="jule_room" tableName="desk_config" domainObjectName="DeskConfig" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="true"
               selectByExampleQueryId="false">
            <property name="useActualColumnNames" value="true"/>
            <property name="selectAllOrderByClause" value="id asc"/>
        </table>
    </context>
</generatorConfiguration>